<!DOCTYPE html>
<html>
  <head>
    <title>EventApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header class = "navbar navbar-default bg-dark navbar-dark" role = "navigation" >
        <div class = "container">
          <div class = "navbar-header">
              <%= link_to "Event App" , root_path, class: "navbar-brand" %>
          </div>
        </div>
    </header>
    <div class = "container mt-4">
      <%= yield %>
    </div>
  </body>
</html>
